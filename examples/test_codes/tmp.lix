have set s := {1, 2, 3}
have set t := x R:
	x > 1

claim:
	forall x s:
		x $in t
	prove:
		forall x s:
			x > 1

claim:
	forall x s:
		x $in t
	prove_by_contradiction:
		forall x s:
			x > 1
		x $in s
		x $in t

