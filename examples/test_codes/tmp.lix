prop point_on_line(a point, l line)
prop point_on_plane(a point, p plane)
prop line_on_plane(l line, p plane)

know prop two_points_on_line_then_line_on_plane(a point, b point, l line, p plane):
        a != b
        $point_on_line(a, l)
        $point_on_line(b, l)
        $point_on_plane(a, p)
        $point_on_plane(b, p)
        iff:
            $line_on_plane(l, p)

obj a point, b point, l line, p plane:
    a != b
    $point_on_plane(a, p)
    $point_on_plane(b, p)
    $point_on_line(a, l)
    $point_on_line(b, l)
$two_points_on_line_then_line_on_plane(a, b, l, p)
#$line_on_plane(l, p)
