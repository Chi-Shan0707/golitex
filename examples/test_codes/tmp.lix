prove:
    let a, b, c, d R:
        c = d
        a + c = b + d
    $cancel_general(a, b, c, d)
    a = b

prove:
    let a, b, c, d R:
        c = d
        a - c = b - d
    $cancel_general(a, b, c, d)
    a = b

prove:
    let a, b, c, d R:
        c = d
        a * c = b * d
        c != 0
    $cancel_general(a, b, c, d)
    a = b

prove:
    let a, b, c, d R:
        c = d
        a / c = b / d
        c != 0
    $cancel_general(a, b, c, d)
    a = b