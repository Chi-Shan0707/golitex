claim:
    forall a N:
        a $in N
    prove:
        a $in N

claim:
    forall a, b, c N:
        a + b = c
        =>:
            a + b = c
    prove:
        a + b = c

claim:
    1 = 1
    prove:
        1 $in N

prove:
    1 + 1 = 2
    claim:
        1 + 1 = 2
        prove:
            1 + 1 = 2