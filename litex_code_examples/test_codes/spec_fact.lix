# exist st => exist
prove:
    exist_prop a A st p(x X):
        $q(a, x)

    know:
        exist 1 st $p(a)
    exist $p(a)

# forall not => not exist
prove:
    exist_prop a A st P():
        $p(a)

    know:
        forall x A:
            not $p(x)
    not $p(a)

    not exist $P()

# not exist => forall not
prove:
    exist_prop a A st P():
        $p(a)

    know not exist $P()

    forall x A:
        not $p(x)
