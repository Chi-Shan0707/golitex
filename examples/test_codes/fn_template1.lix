"""
# know ... $in fn_template
fn_template f_template(x Z) Z:
    dom:
        x >= 0
    then:
        f_template(x) = x + 1  
prove:
    obj m f_template
    m(0) = 0 + 1

# prove xxx $in fn_template
fn f2(x Z) Z:
    dom:
        x >= -1
    then:
        f2(x) = x + 1

prove:
    f2 $in f_template

# fn name as fn name
fn f3(x Z) f_template:
    dom:
        x > 0 
prove:
    know f3(1) $in f_template # true, because return value of f3 is $in f_template。这是要写的，否则找不到f3(1) 的性质
    f3(1)(2) = 2 + 1
"""

prove:
    fn_template f_t(x Z) Z:
        dom:
            x >= 0
        then:
            f_t(x) = x + 10

    fn_template f_t2(x Z) f_t:
        dom:
            x >= 0

    fn f(x Z) f_t2:
        dom:
            x >= 0

    f(0) $in f_t2
    f(0)(1) $in f_t
    #f(0)(1)(2) = 2 + 10
    f(-10)(1)(2) = 2 + 10