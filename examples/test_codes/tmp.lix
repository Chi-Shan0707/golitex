fn add_product(x, y set_product(N, N)) set_product(N, N):
    x[0] + y[0] = add_product(x, y)[0]
    x[1] + y[1] = add_product(x, y)[1]

(1,2) = (1,2)


proj(0, add_product((1,2), (3,4))) = proj(0, (1,2)) + proj(0, (3,4))
