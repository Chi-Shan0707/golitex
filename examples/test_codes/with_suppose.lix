prop is_group(G set, op fn(G, G)G, e G, inv fn(G)G):
    forall a G, b G, c G:
        op(a, op(b, c)) = op(op(a, b), c)
        op(a, e) = a
        op(a, inv(a)) = e

suppose is_group(G, op, e, inv):
    forall a G, b G, c G:
        op(a, op(b, c)) = op(op(a, b), c)
        op(a, e) = a
        op(a, inv(a)) = e
