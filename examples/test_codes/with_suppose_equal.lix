prop is_group(G set, op fn(G, G)G, e G, inv fn(G)G):
    forall a G, b G, c G:
        op(a, op(b, c)) = op(op(a, b), c)
        op(a, e) = a
        op(a, inv(a)) = e

prop properties_about_group(G set, op fn(G, G)G)

suppose is_group(G_, op_, e_, inv_):
    know op_ = e_

obj G set
fn op(x G, y G)G
fn inv(x G)G
obj e G

know $is_group(G, op, e, inv)

with is_group(G, op, e, inv):
    op = e