fn f(x, y R) R:
    x > 1
    y > 1
    then:
    	f(x, y) = x + y

prop p(x, y R):
    x > 1
    y > 1
    iff:
        p(x, y) = x + y

forall x, y R:
    x > 1
    y > 1
    then:
        p(x, y) = x + y

forall x, y R:
    dom:
        x > 1
        y > 1
    then:
        p(x, y) = x + y
    iff:
        p(x, y) = x + y

obj x R, y R:
    x > 0

exist_prop x, y R st q(z R):
    z > 0
    iff:
    	z = x + y