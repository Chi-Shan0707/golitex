prop is_group(G set, mul fn(G, G)G, inv fn(G)G, zero G)

know suppose $is_group(G, mul, inv, zero):
     forall a G:
        mul(a, inv(a)) = zero
        mul(inv(a), a) = zero
        mul(a, zero) = a
        mul(zero, a) = a

obj G set
fn mul(a G, b G) G
fn inv(a G) G
obj zero G

know:
    $is_group(G, mul, inv, zero)

obj a G
with $is_group(G, mul, inv, zero):
    mul(a, inv(a)) = zero

fn f(a G, b G) G
prop p(a G, b G)

know suppose $is_group(G, mul, inv, zero):
    $p(a, zero)
    #f(zero, zero) = zero

with $is_group(G, mul, inv, zero):
    $p(a, zero)
    #f(zero, zero) = zero