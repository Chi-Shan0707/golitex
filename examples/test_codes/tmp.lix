fn exclude(s1, s2 set) set:
    exclude(s1, s2) := x s1:
        not x $in s2

# forall x exclude(s1, s2):
forall s1, s2 set, x exclude(s1, s2):
    x $in s1
    not x $in s2

know forall s1 finite_set, s2 finite_set:
	forall x s2:
		x $in s1
    then:
        len(s1) - len(s2) = len(exclude(s1, s2))
