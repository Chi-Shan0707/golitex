prove:
    prop is_group(G set, op fn(G, G)G, e G, inv fn(G)G):
        forall a G:
            op(a, inv(a)) = e
            op(inv(a), a) = e
            op(a, e) = a
            op(e, a) = a
            op(a, op(b, c)) = op(op(a, b), c)

    when $is_group(G, op, e, inv):
        forall a G:
            op(a, inv(a)) = e
            op(inv(a), a) = e
            op(a, e) = a
            op(e, a) = a
            op(a, op(b, c)) = op(op(a, b), c)
