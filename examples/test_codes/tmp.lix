fn finite_seq_sum(n N_pos, a finite_seq(R, n), k N) R:
    dom:
        k < n

know:
    forall n N_pos, a finite_seq(R, n), k N:
        k < n - 1
        then:
        	finite_seq_sum(n, a, k+1) = finite_seq_sum(n, a, k) + a(k+1)
    finite_seq_sum(n, a, 0) = a(0)

fn finite_seq_product(n N_pos, a finite_seq(R, n), k N) R:
    dom:
        k < n

know:
    forall n N_pos, a finite_seq(R, n), k N:
        k < n - 1
        then:
        	finite_seq_product(n, a, k+1) = finite_seq_product(n, a, k) * a(k+1)
    finite_seq_product(n, a, 0) = a(0)

know @AM_GM_inequality(n N_pos, a finite_seq(R, n)):
	forall k N:
    	k < n
        then:
            a(k) >= 0
    then:
    	finite_seq_sum(n, a, n-1) / (n+1) >= (finite_seq_product(n, a, n-1))^(1/(n+1))

know @AM_GM_inequality_equal_cond(n N_pos, a finite_seq(R, n)):
    forall k N:
    	k < n
        then:
            a(k) >= 0
    then:
        seq_sum(a, n) / (n+1) = (seq_product(a, n))^(1/(n+1))
    iff:
        forall k N, k2 N:
            k < n
            k2 < n
            then:
                a(k) = a(k2)

prove:
    obj a finite_seq(R, 10):
        forall k N:
            k < 10
            then:
            	a(k) > 0
        
    	