fn_template self_seq(s set):
    fn (n N_pos) s

fn self_sum(a self_seq(R), n N_pos, m N_pos) R:
    n <= m
    then:
        self_sum(a, n, m + 1) = self_sum(a, n, m) + a(m + 1)

know forall a self_seq(R), n N_pos => self_sum(a, n, n) = a(n)

prop result(a self_seq(R), m N_pos): m > 1 <=> self_sum(a, 1, m) = a(1) + self_sum(a, 2, m)

claim:
    forall a self_seq(R):
        forall m N_pos: m > 1 => $result(a, m)
    prove:
        self_sum(a, 1, 1) = a(1)
        =:
            self_sum(a, 1, 2)
            self_sum(a, 1, 1) + a(2)
            self_sum(a, 1, 1) + self_sum(a, 2, 2)
            a(1) + self_sum(a, 2, 2)
        $result(a, 2)
        know forall a self_seq(R), m N_pos: m > 1, $result(a, m) => $result(a, m + 1)
        # prove_by_math_induction($result(a, n), 1, 2)
        know forall m N_pos: m > 1 => m >=2, $result(a, m)

