fn_template f(x, y R):
    dom:
    	x > 1
    fn (a, b R) R:
    	dom:
        	a = x
        then:
        	f(a, b) = y

fn_template g(x, y R):
    fn (a, b R) R:
        dom:
        	a = x
        then:
        	g(a, b) = y

obj f g(1,2)

f $in g(1,2)

forall a,b R:
	a = 1
    then:
    	f(a, b) = 2

