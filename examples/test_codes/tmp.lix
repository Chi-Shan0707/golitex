claim:
    forall a R => a^2 >= 0
    prove:
        prove_in_each_case:
            or:
                a > 0
                a = 0
                a < 0
            =>:
                a^2 >= 0
            prove:
                a * a = a ^ 2
                a ^ 2 > 0
                a ^ 2 >= 0
            prove:
                =(0, 0^2, a ^ 2, a * a)
                0 >= 0
                a^2 >= 0
            prove:
                a ^ 2 = (-a) ^ 2
                -a > 0
                (-a) ^ 2 > 0
                (-a) ^ 2 >= 0
    