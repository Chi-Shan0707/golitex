fn zero_func(x Z)Z:
    x = 0

exist_prop f fn(Z)Z st exist_f_is_always_zero():
    forall x Z:
        f(x) = 0

exist zero_func st $exist_f_is_always_zero()

have f st $exist_f_is_always_zero()
