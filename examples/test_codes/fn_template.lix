// know ... $in fn_template
fn_template f_template(x Z) Z:
    dom:
        x >= 0
    then:
        f_template(x) = x + 1  

prove:
    obj m f_template
    m(0) = 0 + 1

// prove xxx $in fn_template
fn f2(x Z) Z:
    dom:
        x >= -1
    then:
        f2(x) = x + 1

prove:
    obj m f2
    m(0) = 0 + 1


